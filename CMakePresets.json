{
  "version": 6,
  "include": [
    ".env.json"
  ],
  "configurePresets": [
    {
      "name": "conda-shared",
      "hidden": true,
      "inherits": [
        "env-vars"
      ],
      "warnings": {
        "dev": false
      },
      "environment": {
        "PREFIX": "$env{MED_PREFIX}"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "MED_MEDINT_TYPE": "int",
        "MEDFILE_INSTALL_DOC": "OFF",
        "MEDFILE_BUILD_PYTHON": "ON",
        "MEDFILE_BUILD_SHARED_LIBS": "ON",
        "MEDFILE_BUILD_STATIC_LIBS": "OFF"
      }
    },
    {
      "name": "gcc-ninja-debug",
      "description": "Using gcc",
      "generator": "Ninja",
      "inherits": "conda-shared",
      "hidden": false,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_PREFIX_PATH": "$env{PREFIX}:$env{PREFIX}/include:$env{PREFIX}/lib:$env{PREFIX}/bin",
        "CMAKE_FIND_ROOT_PATH": "$env{PREFIX}",
        "CMAKE_INSTALL_PREFIX": "$env{INSTALL_PREFIX}"
      }
    }
  ]
}